   clipchildren 0
   ignorepointer 1
   position 6 2
   size 150 16
   valign center_ref
   hexactpos 1
   vexactpos 1
   hexactsize 1
   vexactsize 1
   scriptclass "ViewBinding"
   text "Caption!"
   font "gui/fonts/etelkatextpro"
   "text offset" 3 0
