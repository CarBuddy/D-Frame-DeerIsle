     scriptclass "ViewBinding"
     style EmptyHighlight
     switch toggle
     {
      TextWidgetClass DropdownText {
       clipchildren 1
       ignorepointer 1
       size 1 12
       halign center_ref
       valign center_ref
       hexactpos 1
       vexactpos 1
       hexactsize 0
       vexactsize 1
       scaled 1
       scriptclass "ViewBinding"
       style Normal
       text "Info"
       font "gui/fonts/sdf_MetronLight24"
