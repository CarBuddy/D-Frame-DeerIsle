         hexactsize 1
         vexactsize 1
         image0 "set:ccgui_enforce image:CrScrollV"
         mode blend
         "src alpha" 1
         "no wrap" 0
         stretch 1
         "flip v" 1
         filter 1
        }
       }
      }
     }
     {
      ScriptParamsClass {
       Relay_Command "DropdownPrefabExecute"
      }
     }
    }
   }
  }
  ScrollWidgetClass DropdownScroll {
   ignorepointer 1
