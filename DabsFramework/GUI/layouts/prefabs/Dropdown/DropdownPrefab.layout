       "exact text" 0
       "size to text h" 0
       "size to text v" 0
       "text valign" center
       {
        ScriptParamsClass {
         Binding_Name "Value"
        }
       }
      }
      FrameWidgetClass DropdownIconFrame {
       clipchildren 0
       inheritalpha 0
       ignorepointer 1
       position 5 0
       size 15 15
       halign right_ref
       valign center_ref
       hexactpos 1
       vexactpos 1
       hexactsize 1
       vexactsize 1
