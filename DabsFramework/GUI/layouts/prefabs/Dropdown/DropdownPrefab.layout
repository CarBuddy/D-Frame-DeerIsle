FrameWidgetClass DropdownPrefab {
 position 0 0
 size 500 18
 halign center_ref
 valign center_ref
 hexactpos 1
 vexactpos 1
 hexactsize 1
 vexactsize 1
 {
  TextWidgetClass DropdownPrefabCaption {
   clipchildren 0
   ignorepointer 1
   position 6 2
   size 150 16
   valign center_ref
   hexactpos 1
   vexactpos 1
   hexactsize 1
   vexactsize 1
   scriptclass "ViewBinding"
   text "Caption!"
   font "gui/fonts/etelkatextpro"
   "text offset" 3 0
   "exact text" 0
   "size to text h" 0
   "size to text v" 0
   "text valign" center
   {
    ScriptParamsClass {
     Binding_Name "Caption"
    }
   }
  }
  PanelWidgetClass DropdownOutline {
   ignorepointer 1
   color 0.7059 0.7059 0.7059 1
   position 25 0
   size 150 18
   halign right_ref
   valign center_ref
   hexactpos 1
   vexactpos 1
   hexactsize 1
   vexactsize 1
   style PanelOutline_1px_BlackBackground
   {
    ButtonWidgetClass DropdownPrefabButton {
     clipchildren 1
     inheritalpha 0
     ignorepointer 0
     position 0 0
     size 150 18
     halign right_ref
     valign center_ref
     hexactpos 0
     vexactpos 0
     hexactsize 1
     vexactsize 1
     scriptclass "ViewBinding"
     style Empty
     switch toggle
     {
      TextWidgetClass DropdownText {
       clipchildren 1
       ignorepointer 1
       size 1 16
       halign center_ref
       valign center_ref
       hexactpos 1
       vexactpos 1
       hexactsize 0
       vexactsize 1
       scaled 1
       scriptclass "ViewBinding"
       style Normal
       text "Info"
       font "gui/fonts/sdf_MetronLight24"
       "text offset" 10 0
       "exact text" 0
       "size to text h" 0
       "size to text v" 0
       "text valign" center
       {
        ScriptParamsClass {
         Binding_Name "Value"
        }
       }
      }
      FrameWidgetClass DropdownIconFrame {
       clipchildren 0
       inheritalpha 0
       ignorepointer 1
       position 5 0
       size 15 15
       halign right_ref
       valign center_ref
       hexactpos 1
       vexactpos 1
       hexactsize 1
       vexactsize 1
       fixaspect inside
       {
        ImageWidgetClass DropdownExpandIcon {
         visible 1
         clipchildren 0
         inheritalpha 1
         ignorepointer 1
         position 0 0
         size 15 15
         halign center_ref
         valign center_ref
         hexactpos 0
         vexactpos 1
         hexactsize 1
         vexactsize 1
         image0 "set:ccgui_enforce image:HudMantaLight"
         mode blend
         "src alpha" 1
         "no wrap" 1
         stretch 1
         "flip v" 1
         filter 1
        }
        ImageWidgetClass DropdownCollapseIcon {
         visible 0
         clipchildren 0
         inheritalpha 1
         ignorepointer 1
         position 0 0
         size 15 15
         halign center_ref
         valign center_ref
         hexactpos 0
         vexactpos 1
         hexactsize 1
         vexactsize 1
         image0 "set:ccgui_enforce image:HudMantaLight"
         mode additive
         "src alpha" 1
         "no wrap" 1
         stretch 1
         "flip u" 0
         "flip v" 0
         filter 1
        }
       }
      }
     }
     {
      ScriptParamsClass {
       Relay_Command "DropdownPrefabExecute"
      }
     }
    }
   }
  }
 }
}