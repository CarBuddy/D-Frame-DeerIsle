   "size to text h" 0
   "size to text v" 0
   "text valign" center
   {
    ScriptParamsClass {
     Binding_Name "Caption"
    }
   }
  }
  PanelWidgetClass DropdownOutline {
   ignorepointer 1
   color 0.7059 0.7059 0.7059 1
   position 25 0
   size 200 16
   halign right_ref
   valign center_ref
   hexactpos 1
   vexactpos 1
   hexactsize 1
   vexactsize 1
   style PanelOutline_1px_BlackBackground
   {
